<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
  <title>GettingStarted</title>
  <%= javascript_include_tag 'application' %>
  <%= stylesheet_link_tag  '//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css' %>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag  '//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js' %>
  <link href='https://fonts.googleapis.com/css?family=Anton' rel='stylesheet' type='text/css'>
  <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,400italic,700,700italic,300,300italic' rel='stylesheet' type='text/css'>
  <%= csrf_meta_tags %>
</head>
<body>

	<nav class="navbar navbar-static-top">
	  <div class="container">
	    <ul class="nav navbar-nav">
	      <li class="<%= 'active' if current_page?(root_path) %>">
	        <a href="<%= root_path %>"><span class="glyphicon glyphicon-home"></span> Home</a>
	      </li>
	      <li class="<%= 'active' if current_page?(about_path) %>">
	        <a href="<%= about_path %>"></span> About</a>
	      </li>
	    </ul>
	  </div>
	</nav>

	<%= render 'shared/header' %>

	<%= yield %>

	<%= render 'shared/footer' %>

</body>
</html>

